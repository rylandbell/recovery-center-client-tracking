{
  "name": "wasatch",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build-watch": "watchify -t [babelify] -x react -x react-dom -x redux -x expect -e src/**/main*.js* -e src/misc/*.js -o public/javascripts/bundle.js",
    "build-vendor": "browserify -t [babelify] -r react -r react-dom -r redux -r expect -o /public/javascripts/vendor.js",
    "sass-watch": "sass --watch scss:css",
    "test": "mocha --compilers js:babel-register ./test",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "basic-auth": "^1.0.3",
    "basic-auth-connect": "^1.0.0",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-logger": "0.0.3",
    "jade": "~1.11.0",
    "minimatch": "^3.0.2",
    "moment": "^2.14.1",
    "morgan": "~1.6.1",
    "redux-thunk": "^2.1.0",
    "request": "^2.71.0",
    "serve-favicon": "~2.3.0"
  },
  "engines": {
    "node": "~4.2.6",
    "npm": "~2.14.12"
  },
  "devDependencies": {
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.11.6",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "browserify-shim": "^3.8.12",
    "chai": "^3.5.0",
    "deep-freeze": "0.0.1",
    "eslint": "^3.3.1",
    "eslint-plugin-react": "^6.1.2",
    "expect": "^1.20.2",
    "grunt": "~0.4.5",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^19.0.0",
    "grunt-jscs": "^3.0.0",
    "grunt-puglint": "^0.1.3",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^2.5.3",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "redux": "^3.5.2",
    "uglify-js": "^2.7.3"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-object-assign"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "mocha": true,
      "jquery": true
    },
    "globals": {
      "moment": false
    },
    "plugins": [
      "react"
    ],
    "react": {
      "pragma": "React",
      "version": "15.0"
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "react/prop-types": 0,
      "react/display-name": 0
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "$",
    "three": "global:THREE"
  },
  "browser": {
    "moment": "./src/vendor/moment.min.js",
    "jquery": "./src/vendor/jquery-1.11.1.min.js"
  }
}
