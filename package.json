{
  "name": "wasatch",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "browserify -t [babelify] -x react -x react-dom -x redux -x expect src/messaging/main.jsx -o public/javascripts/messaging.js",
    "build-vendor": "browserify -t [babelify] -r react -r react-dom -r redux -r expect -o /public/javascripts/vendor.js",
    "build-watch": "watchify -t [babelify] -x react -x react-dom -x redux -x expect src/messaging/main.jsx -o public/javascripts/messaging.js",
    "sass-watch": "sass --watch scss:css",
    "test": "mocha --compilers js:babel-register ./test",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "basic-auth": "^1.0.3",
    "basic-auth-connect": "^1.0.0",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-logger": "0.0.3",
    "jade": "~1.11.0",
    "minimatch": "^3.0.2",
    "morgan": "~1.6.1",
    "request": "^2.71.0",
    "serve-favicon": "~2.3.0"
  },
  "engines": {
    "node": "~4.2.6",
    "npm": "~2.14.12"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.11.6",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "deep-freeze": "0.0.1",
    "expect": "^1.20.2",
    "grunt": "^1.0.1",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-jscs": "^3.0.0",
    "grunt-puglint": "^0.1.3",
    "mocha": "^2.5.3",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "redux": "^3.5.2"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  }
}
